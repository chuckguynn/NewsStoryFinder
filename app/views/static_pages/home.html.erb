<p id="notice"><%= notice %></p>
<div class="header">
	<h3><%= current_user.username %> Story's</h3>
</div>

<% current_user.stories.each do |story| %>
<div class="well well-small">
<strong><%= story.title %></strong> |
<%= link_to time_ago_in_words(story.created_at) + " ago", story %><br />
<%= link_to 'Edit', edit_story_path(story), params: :id %> |
<%= link_to 'Remove', story, method: :delete, data: { confirm: 'Are you sure?' } %> 
</div>
<% end %>
<h5><%= link_to "Edit Your Account", edit_user_registration_path %></h5>